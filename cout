#include<iostream>
#include<cstdio>
using namespace std;
int main() {
int  a=3,b=1,c=0,d=-1;
cout<<++a<<'\t'<<b++<<'\t'<<c+++ ++c<<'\t'<<d+++ ++d+d++ +d<<endl;
cout<<b<<'\t'<<c<<'\t'<<d<<endl ;
cout <<"\\\"<<endl;"<<endl ;
cout<<"\"\""<<"\"<<"<<'\"' << "<<"<<endl;
d=0;
printf( "%d \n",/*d);(*/d+++ ++d+d++);
int i=10;
printf("%d,%d",i--,i--);

}